@model CovidBilgilendirme.Models.classes.Tester

@{
    ViewBag.Title = "GetTester";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">

    <div class="col-lg-12">

        <h3 class="page-header"><i class="icon_profile"></i>Test Yaptıranların Bilgisini Güncelleme</h3>

        <ol class="breadcrumb">

            <li><i class="fa fa-home"></i><a href="~/Admin/Index/">Home</a></li>

            <li><i class="icon_profile"></i>Test Yaptıranların Bilgisini Güncelleme</li>

        </ol>

    </div>

</div>



@using (Html.BeginForm("UpdateTester", "Admin", FormMethod.Post))

{

    @Html.HiddenFor(x => x.ID)

    <br />



    @Html.LabelFor(x => x.FirstName)

    @Html.TextBoxFor(x => x.FirstName, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.LastName)

    @Html.TextBoxFor(x => x.LastName, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.TcNo)

    @Html.TextBoxFor(x => x.TcNo, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.BirthYear)

    @Html.TextBoxFor(x => x.BirthYear, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.Email)

    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.Phone)

    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })

    <br />



    @Html.LabelFor(x => x.Result)

    @Html.TextBoxFor(x => x.Result, new { @class = "form-control" })

    <br />



    <button class="btn btn-success" style="font-weight:bold">Güncelle</button>



}

