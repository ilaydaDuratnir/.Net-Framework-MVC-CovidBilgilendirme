@model CovidBilgilendirme.Models.classes.Table_base

@{
    ViewBag.Title = "Tables";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">

    <div class="col-lg-12">

        <h3 class="page-header"><i class="icon_profile"></i>Tablolar</h3>

        <ol class="breadcrumb">

            <li><i class="fa fa-home"></i><a href="~/Admin/Index/">Home</a></li>

            <li><i class="icon_profile"></i>Tablolar</li>

        </ol>

    </div>

</div>

<div class="row">

    <div class="col-lg-6">

        <section class="panel">

            <header class="panel-heading">

                Pozitifler

            </header>

            <table class="table table-striped table-advance table-hover">

                <tbody>

                    <tr>

                        <th><i class="icon_profile"></i>İsim</th>

                        <th><i class="icon_profile"></i>Soyisim</th>

                        <th> <i class="icon_profile"></i>Tc No</th>

                        <th><i class="icon_calendar"></i> Doğum Yılı</th>

                        <th><i class="icon_mail_alt"></i> Email</th>

                        <th><i class="icon_mobile"></i> Telefon No</th>

                    </tr>

                    @foreach (var p in Model.Positives)

                    {

                        <tr>

                            <td>@p.FirstName</td>

                            <td>@p.LastName</td>

                            <td>@p.TcNo</td>

                            <td>@p.BirthYear</td>

                            <td>@p.Email</td>

                            <td>@p.Phone</td>



                        </tr>



                    }



                </tbody>

            </table>

        </section>

    </div>

    <div class="col-lg-6">

        <section class="panel">

            <header class="panel-heading">

                Negatifler

            </header>

            <table class="table table-striped table-advance table-hover">

                <tbody>

                    <tr>

                        <th><i class="icon_profile"></i>İsim</th>

                        <th><i class="icon_profile"></i>Soyisim</th>

                        <th> <i class="icon_profile"></i>Tc No</th>

                        <th><i class="icon_calendar"></i> Doğum Yılı</th>

                        <th><i class="icon_mail_alt"></i> Email</th>

                        <th><i class="icon_mobile"></i> Telefon No</th>

                    </tr>

                    @foreach (var n in Model.Negatives)

                    {

                        <tr>

                            <td>@n.FirstName</td>

                            <td>@n.LastName</td>

                            <td>@n.TcNo</td>

                            <td>@n.BirthYear</td>

                            <td>@n.Email</td>

                            <td>@n.Phone</td>



                        </tr>



                    }



                </tbody>

            </table>

        </section>

    </div>

</div>

